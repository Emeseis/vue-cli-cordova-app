{
  "name": "vue-cli-cordova-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-serve-android",
    "build": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-android",
    "prepare": "vue-cli-service cordova-prepare"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.11.0",
    "core-js": "^3.44.0",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.7.1",
    "vue-cli-plugin-cordova": "^2.4.1",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "webpack-plugin-vuetify": "^3.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "cordova": "readonly",
      "Camera": "readonly",
      "mltext": "readonly"
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "overrides": {
    "@vue/cli-shared-utils": {
      "@achrinza/node-ipc": "9.2.9"
    }
  }
}
